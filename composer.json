    {
      "name": "Alex Porcelanosa",
      "email": "porcelanosam@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "vova07/yii2-imperavi-widget": "*",
